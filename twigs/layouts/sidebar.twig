{% include('includes/head.twig') %}
<body {{ wp.body_class }} >
   <div id="wrap">
    {% include('includes/header.twig') %}
    {% block sidecontent %}
    <div class="main">
     <div class="row" data-equalizer>
         <section  data-equalizer-watch class="post small-12 large-8 medium-8 columns">
         {% block content %}{% endblock %}
         </section>
         <section  data-equalizer-watch class="sidebar small-12 large-4 medium-4 columns">
           {% set sidebar = wp.dynamic_sidebar('sidebar-primary') %}
         </section>
     </div>
    </div>
    {% endblock %}
    {% include('includes/footer.twig') %}
    {{ wp.wp_footer }}
   </div>
   <script>
     (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
     (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
     m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
     })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

     ga('create', 'UA-42301403-1', 'auto');
     ga('send', 'pageview');

   </script>   
</body>
</html>
